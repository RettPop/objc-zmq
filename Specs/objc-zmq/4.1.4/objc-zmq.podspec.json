{
  "name": "objc-zmq",
  "version": "4.1.4",
  "summary": "Objective-C binding for ZeroMQ",
  "description": "Bundled with ZeroMQ 4.1.4 library.\n\nThis is an Objective-C version of the reference ZeroMQ object-oriented C API. It follows the guidelines laid out by the official \"Guidelines for ZeroMQ bindings\".",
  "homepage": "https://github.com/rettpop/objc-zmq",
  "license": "MIT",
  "authors": {
    "Rogerio Candeias": "rettpop@gmail.com"
  },
  "source": {
    "git": "https://github.com/rettpop/objc-zmq.git",
    "tag": "4.1.4"
  },
  "platforms": {
    "ios": "7.0"
  },
  "ios": {
    "deployment_target": "7.0"
  },
  "requires_arc": true,
  "source_files": [
    "Classes/cppzmq/*.hpp",
    "Classes/objc-zmq/*.{h,m}",
    "Classes/zeromq/*.h"
  ],
  "public_header_files": "Classes/**/*.{h,hpp}",
  "vendored_libraries": [
    "Library/zeromq-4.1.4/static/libzmq.a",
    "Library/libsodium-1.0.12/static/libsodium.a"
  ],
  "libraries": "c++"
}
